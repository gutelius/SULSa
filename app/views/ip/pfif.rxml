@xml.instruct! 


xml.tag! "pfif:pfif", "xmlns:pfif" =>"http://zesty.ca/pfif/1.1","xmlns:xsi"=>"http://www.w3.org/2001/XMLSchema-instance","xsi:schemaLocation"=>"http://zesty.ca/pfif/1.1 http://zesty.ca/pfif/1.1/pfif-1.1.xsd" do
  xml.tag! "pfif:person" do
  xml.home_country(@country[0].country_name)
  xml.home_city(@country[0].city)
    
  xml.home_state(@country[0].region_name)
  xml.home_zip(@country[0].zipcode) 
 
   
    
  end
end